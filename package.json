{
  "name": "amplication",
  "scripts": {
    "build": "nx run-many --target=build --exclude=admin-template",
    "test": "nx run-many --target=test --exclude=admin-template",
    "test:e2e": "nx run-many --target=test:e2e",
    "check-format": "nx run-many --target=format",
    "format": "nx run-many --target=format",
    "lint": "nx run-many --target=lint --exclude=admin-template",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes",
    "prisma:generate": "nx prisma:generate @amplication/prisma-db",
    "migrate:save": "nx migrate:save @amplication/prisma-db",
    "migrate:up": "nx migrate:up @amplication/prisma-db",
    "migrate:rollback": "nx migrate:rollback @amplication/prisma-db",
    "generate-graphql-schema": "nx generate-graphql-schema @amplication/server",
    "check-graphql-schema": "nx check-graphql-schema @amplication/server",
    "generate-graphql-code": "graphql-codegen",
    "generate": "run-s generate-graphql-schema generate-graphql-code",
    "build-storybook": "nx build-storybook @amplication/design-system",
    "generate-test-data-service": "nx generate-test-data-service @amplication/data-service-generator",
    "setup:dev": "ts-node ./script/setup.ts",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d"
  },
  "dependencies": {
    "lerna": "5.3.0",
    "ora": "5.4.0",
    "react-query": "^3.39.1",
    "semver": "7.3.5",
    "winston": "3.3.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.3",
    "@graphql-codegen/typescript": "2.4.1",
    "@nrwl/nx-cloud": "latest",
    "@types/jest": "27.0.2",
    "@types/ora": "3.2.0",
    "@types/semver": "7.3.9",
    "graphql": "15.3.0",
    "npm-run-all": "4.1.5",
    "nx": "14.5.1",
    "prettier": "2.0.5",
    "ts-jest": "27.0.5",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": "^16.3.0 || ^14.0.0",
    "npm": "^8.1.0 || ^7.3.0"
  },
  "bugs": {
    "url": "https://github.com/amplication/amplication/issues"
  }
}
